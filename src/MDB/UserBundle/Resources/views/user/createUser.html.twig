{%extends 'MDBUserBundle:User:base.html.twig'%}
{%block title%}Maak een nieuwe user aan{%endblock%}
{%block content%}
  {{dump(form)}}
  {%if form is defined%}
    {{ form_start(form) }}
    {{form_row(form.nicknaam)}}
    {{form_row(form.voornaam)}}
    {{form_row(form.naam)}}
    {{form_row(form.email)}}
    {{form_row(form.paswoord)}}
    {{form_row(form.registreer)}}
    {% do form.login.setRendered %}
    {{ form_end(form) }}
  {%endif%}
  {%if errors is defined%}
    {%for error in errors%}
      {{error.message}}
    {%endfor%}
  {%endif%}
{%endblock%}

